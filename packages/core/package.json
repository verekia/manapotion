{
  "name": "@manapotion/core",
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "dts": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --ext ts",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "publint": "publint"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.2.1",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "publint": "0.2.7",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "zustand": "5.0.2"
  }
}
