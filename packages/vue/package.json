{
  "name": "@manapotion/vue",
  "version": "0.14.1",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "dts": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "publint": "publint"
  },
  "dependencies": {
    "@manapotion/store": "^0.14.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.0",
    "zustand": ">=4.0.0"
  },
  "devDependencies": {
    "publint": "^0.2.7",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
