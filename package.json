{
  "name": "manapotion-monorepo",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "cd examples/react && npm run dev",
    "dev-vue": "cd examples/vue && npm run dev",
    "clean": "rm -rf packages/*/dist",
    "build": "npm run build -w @manapotion/util -w @manapotion/tailwind -w @manapotion/browser -w @manapotion/store -w @manapotion/react -w @manapotion/r3f -w @manapotion/vue -w manapotion",
    "publint": "npm run publint -ws",
    "pub": "npm run build && npm run publint && lerna publish"
  },
  "devDependencies": {
    "lerna": "^8.1.2"
  }
}
