{
  "name": "manapotion-monorepo",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "example"
  ],
  "scripts": {
    "dev": "cd example && npm run dev",
    "clean": "rm -rf packages/*/dist",
    "build": "npm run build -w @manapotion/util -w @manapotion/browser -w @manapotion/react -w @manapotion/r3f -w manapotion",
    "publint": "npm run publint -ws",
    "pub": "npm run build && npm run publint && lerna publish"
  },
  "devDependencies": {
    "lerna": "^8.1.2"
  }
}
